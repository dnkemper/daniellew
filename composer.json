  "autoload": {
    "psr-4": {
      "Artsci\\": "blt/src/"
    },
    "classmap": ["scripts/composer/ScriptHandler.php"],
    "files": ["load.environment.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Artsci\\Tests\\PHPUnit\\": "tests/phpunit/src/"
    }
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "acquia/blt": true,
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "oomphinc/composer-installers-extender": true,
      "phpstan/extension-installer": true,
      "simplesamlphp/composer-module-installer": true,
      "simplesamlphp/composer-xmlprovider-installer": false
    },
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./web"
      },
      "file-mapping": {
        "[web-root]/robots.txt": false,
        "[web-root]/sites/default/default.services.yml": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/default.services.yml",
          "overwrite": false
        },
        "[web-root]/sites/default/default.settings.php": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/default.settings.php",
          "overwrite": false
        }
      }
    },
    "installer-types": ["npm-asset", "bower-asset"],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "web/libraries/ckeditor5-anchor-drupal": [
        "npm-asset/northernco--ckeditor5-anchor-drupal"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/access_unpublished": {
        "access_unpublished twig extension for displaying hashtag": "https://www.drupal.org/files/issues/2023-05-16/v1.1-access_unpublished-3054536-17.patch"
      },
      "drupal/captcha": {
        "Add captcha to all webforms": "https://www.drupal.org/files/issues/2023-08-08/3380095-add-captcha-to-webforms.patch"
      },
      "drupal/rabbit_hole": {
        "Uncaught TypeError textContent is not a function": "https://www.drupal.org/files/issues/2024-10-31/3476681-rabbit-hole-fix.patch"
      },
      "drupal/core": {
        "Responsive image absolute paths": "patches/responsive-image-absolute-path.patch",
        "HTML tag stripping in rss feeds": "./patches/3445910-issue-with-html-non-breaking-space.patch",
        "[2811043] absolute urls in the default settings.php": "https://www.drupal.org/files/issues/2024-08-09/2811043-147.patch",
        "undefined array key": "https://www.drupal.org/files/issues/2024-04-24/warning-undefined-array-key-1-3443132-2.patch",
        "Allow altering field value query.": "https://www.drupal.org/files/issues/2021-06-10/core-allow_altering_migrate_field_value_query-3218294-2.patch",
        "[AMA_MIGRATE_FIX] Issue #3122649: Derive path alias migrations per entity type (and bundle)": "https://www.drupal.org/files/issues/2021-02-24/core-derive_path_alias_migrations-3122649-41.patch",
        "[AMA_MIGRATE_FIX] Issue #2845340: migrate mapping & messages table names are truncated, can lead to incorrect mapping lookups": "https://www.drupal.org/files/issues/2022-12-20/2845340-63-fix-only-do-not-test.patch",
        "[AMA_MIGRATE_FIX] Issue #3051251: Existing menu links show validation issues on migration": "https://www.drupal.org/files/issues/2023-03-16/core-derive_menu_link_migrations_per_entity_type-3051251-69.patch",
        "[AMA_MIGRATE_FIX] Issue #3167267: MigrateExecutable should catch not only exceptions, but also fatal errors": "https://www.drupal.org/files/issues/2021-11-04/drupal-3167267-36.patch",
        "[AMA_MIGRATE_FIX] Issue #3186449: ContentEntity source plugin shouldn't throw exception when the bundle key is missing": "https://www.drupal.org/files/issues/2020-12-08/core-content_entity_source_exception_when_bundle_is_missing-3186449-2.patch",
        "[AMA_MIGRATE_FIX] Issue #3187419: d7/NodeComplete source plugin adds invalid source \"source_langcode\" for \"content_translation_source\" destination property": "https://www.drupal.org/files/issues/2020-12-09/core-nodecomplete_wrong_source_langcode-3187419-2.patch",
        "[AMA_MIGRATE_FIX] Issue #3187474: Improve source record count of translation migrate source plugins which use the \"i18n_string\" table": "https://www.drupal.org/files/issues/2020-12-14/core-improve_source_record_count_i18n_string-3187474-9.patch",
        "[AMA_MIGRATE_FIX] Issue #3118262: Calling EntityConfig::import() with multiple destination IDs fails": "https://www.drupal.org/files/issues/2021-12-28/core-fix_entityconfigbase-3118262-52--complete.patch",
        "[AMA_MIGRATE_FIX] Issue #3227361: Fix \\Drupal\\migrate\\Plugin\\migrate\\source\\SqlBase::initializeIterator()'s cross-database joining broken when using particular DB/table names": "https://www.drupal.org/files/issues/2021-08-11/3227361-13.patch",
        "PHP warnings in array_flip in EntityStorageBase->loadMultiple()": "https://www.drupal.org/files/issues/2023-01-12/3145190-21.patch",
        "Site extensions don't get rediscovered after drush cr": "https://www.drupal.org/files/issues/2018-07-11/specify_sitepath_cache_rebuild_extension_discovery-2985199-3.patch",
        "Periods in query strings are replaced by underscores": "https://www.drupal.org/files/issues/2020-06-25/fix-mangled-query-parameter-names-2984272-36.patch",
        "Improve migration system performance: statically cache DrupalSqlBase::$systemData": "https://www.drupal.org/files/issues/2022-01-24/3154156-12.patch",
        "[3163299] Ajax exposed filters not working for multiple instances of the same Views block placed on one page": "patches/core-3163299.patch",
        "Fix core view date functionality": "https://www.drupal.org/files/issues/2023-05-19/2868014-136.patch",
        "Fix alt default from getting wiped out": "patches/core--fix-alt-default.patch",
        "PHP8.1 deprecation: str_replace()": "https://www.drupal.org/files/issues/2023-06-07/3294680-22.patch"
      },
      "drupal/default_content": {
        "Predictable order of content import": "https://www.drupal.org/files/issues/2020-10-08/3175870.patch"
      },
      "drupal/diff": {
        "Local tasks for node not visible on diff.revisions_diff route": "https://www.drupal.org/files/issues/2018-03-19/diff-show-local-tasks-2954352-1.patch"
      },
      "drupal/entity_reference_revisions": {
        "[3201705] Orphan purger : if entity's parent is invalid (NULL), dig deeper": "patches/entity-reference-3201705.patch"
      },
      "drupal/linkchecker": {
        "Provide a list of unconfigured but eligible fields": "https://www.drupal.org/files/issues/2022-03-28/3244743-7-unconfigured-fields-list.patch",
        "Linkchecker incorrectly reports permanent redirects on internal linkit links": "https://www.drupal.org/files/issues/2024-09-04/linkchecker-3365483-12.patch",
        "Cover entity types without bundle": "https://www.drupal.org/files/issues/2024-12-14/linkchecker-entity_types_without_bundle.patch-2.1.x.patch",
        "Allow to check link status on demand": "https://www.drupal.org/files/issues/2024-03-08/linkchecker-allow-to-check-links-on-demand-3426268-4.patch",
        "Add option to fallback to GET if HEAD fails": "patches/linkchecker-3334240-use-get-method.patch"
      },
      "drupal/linkit": {
        "Add webform match": "https://www.drupal.org/files/issues/2023-06-13/linkit-webform-matcher-2946234-36.patch"
      },
      "drupal/masquerade": {
        "Generates tokens for masquerade": "https://www.drupal.org/files/issues/2021-01-02/masquerade-masquerade_link_url_token-3190650-2.patch",
        "Remove link destination on unmasquerade": "https://www.drupal.org/files/issues/2021-02-09/3158235-4.patch",
        "Missing icon in toolbar": "https://www.drupal.org/files/issues/2023-05-02/3325299-missing-icon-in-masquerade-12.patch"
      },
      "drupal/moderated_content_bulk_publish": {
        "Remove bulk publish moderated content on node edit form": "patches/moderated_content_bulk_publish.patch"
      },
      "drupal/paragraphs": {
        "Fix migration issue with ignored items": "https://www.drupal.org/files/issues/2023-08-21/3268555-8.patch",
        "Find the correct revision ID of the parent": "https://www.drupal.org/files/issues/2020-07-08/access-controll-issue-3090200-22.patch"
      },
      "drupal/private_content": {
        "Alter private content module perms to work as expected": "patches/private-content-view.patch"
      },
      "drupal/redirect": {
        "Match redirect": "https://www.drupal.org/files/issues/2020-04-13/redirect-n2831605-31.patch"
      },
      "drupal/replicate": {
        "Replicate components in the proper order.": "patches/3392959.patch"
      },
      "drupal/search_api": {
        "Datetime Format Field configuration": "https://www.drupal.org/files/issues/2020-12-08/search_api_datetime_format-add_schema.patch",
        "undefined index #conjunction #3150478": "https://www.drupal.org/files/issues/2021-10-22/undefined_index_conjunction-3150478-19.patch"
      },
      "drupal/smart_date": {
        "Added date granularity": "https://www.drupal.org/files/issues/2023-09-30/smart_date-support_calendar_module-3177760-13.patch"
      },
      "drupal/views_ical": {
        "Fix random errors from D10/PHP8.1": "patches/views_ical.patch"
      }
    }
  },
  "scripts": {
    "post-create-project-cmd": ["blt internal:create-project:init-repo"],
    "post-drupal-scaffold-cmd": [
      "patch -p1 < patches/development_services.patch",
      "patch -p1 < patches/htaccess-simplesaml.patch"
    ],
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "nuke": [
      "rm -rf vendor web/core web/modules/contrib web/profiles/contrib web/themes/contrib",
      "@composer clearcache --ansi",
      "@composer install --ansi"
    ],
    "post-install-cmd": [
      "composer run-script post-install-cmd -d web/modules/custom/washuas"
    ],
    "post-update-cmd": [
      "composer run-script post-install-cmd -d web/modules/custom/washuas"
    ]
  },
  "require-dev": {
    "acquia/coding-standards": "^3.0",
    "drevops/behat-screenshot": "^1.2",
    "kint-php/kint": "^5.0",
    "mglaman/phpstan-drupal": "^1.3",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^1.12",
    "phpstan/phpstan-deprecation-rules": "^1.2",
    "symfony/var-dumper": "^6.3"
  }
}
