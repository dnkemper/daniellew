{{ attach_library('olympian9/datepicker') }}

<div id="calendar1" class="m-calendar datepick-container"
     data-calendar-url="/events">

  <div class="datepick-month">
    <div class="datepick-nav">
      <a class="datepick-cmd-prev" data-calendar-prev>
        <svg>
          <use xlink:href="#arrow-prev"></use>
        </svg>
      </a>
      <b class="datepick-cmd-today" data-calendar-title></b>

      <a class="datepick-cmd-next" data-calendar-next>
        <svg>
          <use xlink:href="#arrow-next"></use>
        </svg>
      </a>
    </div>
    <table>
      <thead>
      <tr>
        <th title="Sunday"><span>S</span></th>
        <th title="Monday"><span>M</span></th>
        <th title="Tuesday"><span>T</span></th>
        <th title="Wednesday"><span>W</span></th>
        <th title="Thursday"><span>T</span></th>
        <th title="Friday"><span>F</span></th>
        <th title="Saturday"><span>S</span></th>
      </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
  </div>
</div>
